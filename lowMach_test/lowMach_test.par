#
# nek parameter file
#
[GENERAL] 
startFrom = #restart.fld 
stopAt = nSteps #endTime
endTime = 1.0
numSteps = 5000

dt = 1e-03
timeStepper = bdf #char #steady
tOrder = 3
variableDt = No
maxCFL = 2.0

writeControl = timeStep #runTime
writeInterval = 500

dealiasing = yes
filtering = no  
filterWeight = 5e-02

userParam01 = 1

[PROBLEMTYPE]
stressFormulation = no
lowMachNumber = yes 
userProperties = yes

[MESH]
motion = no 
#meshVelocity = manual

[PRESSURE]
preconditioner = semg #schwarz
residualTol = 1e-8
residualProj = yes

[VELOCITY]
residualTol = 1e-12
residualProj = no
density = 1
viscosity = 1
#writeToFieldFile = no 

[TEMPERATURE]
rhoCp = 1
conductivity = 1
#conjugateHeatTransfer = yes 
